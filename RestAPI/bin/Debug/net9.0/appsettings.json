{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "IncludeScopes": false,
        "TimestampFormat": "dd.MM.yyyy HH:mm:ss.fff => ",
        "UseUtcTimestamp": false
      }
    }
  },
  "AllowedHosts": "*"
}
